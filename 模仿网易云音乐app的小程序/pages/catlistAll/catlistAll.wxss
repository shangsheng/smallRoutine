/* pages/catlistAll/catlistAll.wxss */
.pages{
  background-color: #fafcfb;
  padding-top: 10px;
}
.catilstAll{
  width: 100%;
  height: 30px;
  text-align: center;
  font-size: 14px;
  line-height: 30px;
  border: 1px solid transparent;
  background-color: #ffffff;
  margin-bottom: 10px;
  box-sizing: border-box;
}
.catilstborder{
  border-color: #e04438;
}
.catilst{
  background-color: #ffffff;
}
.catilst .catilst-ul{
  margin-bottom: 5px;
}
.catilst .catilst-ul .catilst-li{
  width: 25%;
  box-sizing: border-box;
  text-align: center;
  height: 40px;
  line-height: 40px;
  border-bottom: 1px solid #e7e7e7;
  border-left: 1px solid #e7e7e7;
  border-right: 1px solid transparent;
  border-top: 1px solid transparent;
  font-size: 12px;
  float: left;
}
.catilst .catilst-ul .catilst-categories{
  height: 80px;
  line-height: 80px;
}
.catilst .catilst-ul  .catilstborder{
  border-color: #e04438;
}